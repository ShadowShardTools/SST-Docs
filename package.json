{
  "name": "sst-docs",
  "version": "0.1.2",
  "private": true,
  "type": "module",
  "homepage": "https://shadowshardtools.github.io/SST-Docs/",
  "license": "MIT",
  "repository": "github:ShadowShardTools/SST-Docs",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "generate": "run-p generate:blocks generate:prism generate:html",
    "generate:dev": "run-p generate:blocks:full generate:prism:full",
    "generate:blocks": "tsx src/generators/GenerateBlockImports.ts",
    "generate:blocks:full": "tsx src/generators/GenerateBlockImports.ts --full",
    "generate:prism": "tsx src/generators/GeneratePrismLanguages.ts",
    "generate:prism:full": "tsx src/generators/GeneratePrismLanguages.ts --full",
    "generate:html": "tsx src/generators/GenerateConsolidatedHTML.ts",
    "dev": "npm run generate:dev && vite",
    "build": "npm run generate && tsc -b && vite build",
    "start": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "prebuild": "npm run format",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@resvg/resvg-js": "^2.6.2",
    "@shadow-shard-tools/docs-core": "^1.0.1",
    "@splidejs/react-splide": "^0.7.12",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.19",
    "app-root-path": "^3.1.0",
    "chart.js": "^4.5.1",
    "chartjs-node-canvas": "^5.0.0",
    "jszip": "^3.10.1",
    "katex": "^0.16.27",
    "lucide-react": "^0.562.0",
    "lucide-static": "^0.562.0",
    "prismjs": "^1.30.0",
    "react": "^19.2.3",
    "react-chartjs-2": "^5.3.1",
    "react-compare-image": "^3.5.13",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "react-window": "^1.8.9",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.0.3",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.17",
    "baseline-browser-mapping": "^2.9.17",
    "dotenv": "^17.2.3",
    "fast-glob": "^3.3.3",
    "gh-pages": "^6.3.0",
    "happy-dom": "^20.3.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.17"
  }
}
